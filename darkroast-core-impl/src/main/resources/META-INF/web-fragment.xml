<?xml version="1.0" encoding="UTF-8"?>
<web-fragment version="3.0" xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-fragment_3_0.xsd">

    <name>Darkroast</name>

    <distributable/>

    <ordering>
        <before>
            <others/>
        </before>
    </ordering>

    <filter>
        <display-name>Darkroast Context Filter</display-name>
        <filter-name>Darkroast Context Filter</filter-name>
        <filter-class>com.darkroast.servlet.DarkroastDispatchFilter</filter-class>
    </filter>

    <filter-mapping>
        <filter-name>Darkroast Context Filter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>

</web-fragment>
